<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<dependencies>
	<dependency org="axis" name="axis" rev="${axis.version}" />
	<!-- Generated Axis classes uses javax.persistence.Entity -->
	<dependency org="org.hibernate.javax.persistence" name="hibernate-jpa-2.0-api" rev="1.0.1.Final" />
	<!-- Avoid ROO-3163 -->
	<dependency org="org.springframework" name="spring-tx" rev="${spring.version}" />
	<!-- Avoid ROO-770 -->
	<!-- Use maven dependency format to set scope -->
    <dependency>
        <groupId>javax.persistence</groupId>
        <artifactId>persistence-api</artifactId>
        <version>1.0</version>
        <scope>provided</scope>
    </dependency>
</dependencies>