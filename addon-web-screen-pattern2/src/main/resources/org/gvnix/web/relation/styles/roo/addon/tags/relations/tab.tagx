<jsp:root xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:fn="http://java.sun.com/jsp/jstl/functions" xmlns:page="urn:jsptagdir:/WEB-INF/tags/form" xmlns:form="http://www.springframework.org/tags/form" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:spring="http://www.springframework.org/tags" xmlns:field="urn:jsptagdir:/WEB-INF/tags/form/fields" version="2.0">
  <jsp:output omit-xml-declaration="yes"/>
  
  <jsp:directive.attribute name="id" type="java.lang.String" required="true" description="The identifier for this tag (do not change!)"/>

  <c:if test="${empty render or render}">
  
      <script type="text/javascript">dojo.require('dijit.layout.TabContainer');</script>
      <script type="text/javascript">dojo.require('dijit.layout.ContentPane');</script>

      <div id="${id}">
          <jsp:doBody />
          
          <script type="text/javascript">Spring.addDecoration(new Spring.ElementDecoration({elementId : '${id}', widgetType : 'dijit.layout.TabContainer', widgetAttrs : {title: '${id}', style: 'height: 100%', tabPosition: 'top' }})); </script>
      </div>
  </c:if>
</jsp:root>